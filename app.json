{
  "expo": {
    "name": "chat-project",
    "slug": "chat-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/appstore.png",
    "splash": {
      "image": "./assets/splashIcon.png",
      "resizeMode": "contain",
